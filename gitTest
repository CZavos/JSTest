let a = "Alice";
let b = 5;
let c = 20;
let d = a + " bought " + b + " items for $" + c + ".";

console.log(d);
let name = "Alice";
let itemNumber = 5;
let cost = 20;
let declare = name + " bought " + itemNumber + " items for $" + cost + ".";
let itemCost = cost/itemNumber
let iCdeclare = name + " spent $" + itemCost + " per item that she bought.";
console.log(declare);
console.log(iCdeclare);
